<h2>Movies</h2>

    {{#each movies}}

    <div>
        <br>
        <ul><a href="/movies/{{this._id}}"><p>{{this.title}}</p></a></ul>
    </div>

    <a class="edit" href="/movies/{{this.id}}/edit">Edit</a>
     <form action="/movies/{{this.id}}/delete" method="Post">
        <button  class="delete" type="submit">Delete</button>
    </form>
    
    {{/each}}
