<h2>Celebrities</h2>

    {{#each celebrities}}
    <div>
        <br>
        <br>

        <p>Name: {{this.name}}</p> 
        <p>Occupation: {{this.occupation}}</p>
        <p>Catch Phrase: {{this.catchPhrase}}</p>
        
    </div>
  
      <form action="/celebrities/{{this.id}}/delete" method="post">
        <button  class="delete" type="submit">Delete</button>
      </form>
      <br>
    {{/each}}

  
